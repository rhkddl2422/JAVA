package day08;

public class Test08 {

	public static void main(String[] args) {
		
		Drawable [] s = {new Circle(),  new Rectangle()};
		Moverable [] a = {new Circle(),  new Rectangle()};
		
		for(Drawable data : s ) {
			
			data.draw();
			
			((Moverable)data).move();
			
		}
       
	
		
	}

}


interface Drawable{
	
	
	void draw();
	
	
	
}


interface Moverable{
	
	void move();	
	
}



class Circle implements  Drawable, Moverable
{
	
	String name = "Rectengular";
	
	public void move()
	{System.out.println(name+"이동(move)");
		
		
	}
	
	public void draw()
	{
		System.out.println(name+"그리기(draw)");
	}
}
class Rectangle implements  Drawable, Moverable
{
	
	String name ="Rectangular";
}

}