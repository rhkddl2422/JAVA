package report;

public class Student extends Person {
	
	
	String name;
	int age;
	int id;

	
	
	public Student() {
		super();
	

}
	public Student(String name,int age,int id)
	{    
		 this.name=name;
		 this.age=age;
		 this.id=id;
		
	}
	
	@Override
	public void print()
	{
		System.out.printf("이름:%s 나이:%d 학번:%d", this.name,this.age,this.id);
	}
	

}
