package report;

public class Teacher extends Person {
	
	
	String name;
	int age;
	String subject;

	
	
	public Teacher() {
		super();
	

}
	public Teacher(String name,int age,String subject)
	{
		this.name=name;
		this.age=age;
		this.subject=subject;
	}

	@Override 
	public void print()
	{
		System.out.printf("이름:%s 나이:%d,%s%n", this.name,this.age,this.subject);
	}
	
	

}
